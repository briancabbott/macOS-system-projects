<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Disabled</key>
	<true/>
	<key>Label</key>
	<string>org.dovecot.dovecotd</string>
	<key>OnDemand</key>
	<false/>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/libexec/dovecot/mail_preflight</string>
		<string>/usr/sbin/dovecotd</string>
		<string>-F</string>
	</array>
	<key>StandardOutPath</key>
	<string>/Library/Logs/Mail/mail-info.log</string>
	<key>StandardErrorPath</key>
	<string>/Library/Logs/Mail/mail-err.log</string>
	<key>ServiceIPC</key>
	<false/>
	<key>HardResourceLimits</key>
	<dict>
		<key>NumberOfFiles</key>
		<integer>8192</integer>
	</dict>
	<key>SoftResourceLimits</key>
	<dict>
		<key>NumberOfFiles</key>
		<integer>8192</integer>
	</dict>
	<key>PreventsSleep</key>
	<true/>
</dict>
</plist>
