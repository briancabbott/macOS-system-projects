srcdir	= @srcdir@
VPATH	= @srcdir@
SHELL	= /bin/sh

CC	= @CC@
CFLAGS	= @CFLAGS@
CPPFLAGS= -I${srcdir} -I${srcdir}/.. -I. -I.. @INCLUDES@ @CPPFLAGS@

AR	= @AR@
RANLIB	= @RANLIB@

LIB	= libnetbsd.a

OBJS	= @LIBOBJS@


all:	${LIB}

${LIB}: ${OBJS}
	${AR} cr $@ ${OBJS}
	${RANLIB} $@

${OBJS}: ${srcdir}/../tnftp.h ../config.h

.c.o:
	${CC} ${CFLAGS} ${CPPFLAGS} -c $<

install:

clean:
	rm -f ${LIB} ${OBJS}

distclean: clean
	rm -f Makefile
