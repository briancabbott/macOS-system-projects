<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.apple.salearn</string>
	<key>Program</key>
	<string>/etc/mail/spamassassin/sa_learn.pl</string>
	<key>ProgramArguments</key>
	<array>
		<string>sa_learn.pl</string>
	</array>
        <key>StartCalendarInterval</key>
	<dict>
		<key>Hour</key>
		<integer>2</integer>
		<key>Minute</key>
		<integer>15</integer>
	</dict>
	<key>PreventsSleep</key>
	<true/>
</dict>
</plist>
