<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/
PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.apple.auditd</string>
	<key>MachServices</key>
	<dict>
		<key>com.apple.auditd</key>
		<dict>
			<key>HostSpecialPort</key>
			<integer>9</integer>
		</dict>
	</dict>
	<key>TimeOut</key>
	<integer>60</integer>
	<key>Program</key>
	<string>/usr/sbin/auditd</string>
	<key>ProgramArguments</key>
	<array>
		<string>auditd</string>
		<string>-l</string>
	</array>
</dict>
</plist>
