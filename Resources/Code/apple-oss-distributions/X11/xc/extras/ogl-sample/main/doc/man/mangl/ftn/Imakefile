AllTarget($(MAN_PAGES))

list:
	@for i in $(MAN_PAGES); do echo $$i; done;

install_list:
	@for i in $(MAN_PAGES); do echo $(LIBMANDIR)/$$i; done;

install:: install.man

clean::
	$(RM) $(LINK_NAMES) $(ALIAS_NAMES)

InstallMultipleMan($(MAN_PAGES),$(LIBMANDIR))

