#define	ManPageAlias(file,destdir,alias)				@@\
alias.$(MANSUFFIX): file.$(MANSUFFIX)					@@\
	echo .so `basename destdir`/file.$(MANSUFFIX) > alias.$(MANSUFFIX) @@\

