ac_unique_file="move-if-change"
ac_subst_vars='SHELL PATH_SEPARATOR PACKAGE_NAME PACKAGE_TARNAME PACKAGE_VERSION PACKAGE_STRING PACKAGE_BUGREPORT exec_prefix prefix program_transform_name bindir sbindir libexecdir datadir sysconfdir sharedstatedir localstatedir libdir includedir oldincludedir infodir mandir build_alias host_alias target_alias DEFS ECHO_C ECHO_N ECHO_T LIBS build build_cpu build_vendor build_os host host_cpu host_vendor host_os target target_cpu target_vendor target_os INSTALL_PROGRAM INSTALL_SCRIPT INSTALL_DATA TOPLEVEL_CONFIGURE_ARGUMENTS build_subdir host_subdir target_subdir SET_LIB_PATH RPATH_ENVVAR BUILD_PREFIX BUILD_PREFIX_1 configlinks gcc_version_trigger gcc_version tooldir build_tooldir GDB_TK gxx_include_dir libstdcxx_incdir build_configargs configure_build_modules all_build_modules host_configargs configdirs target_configargs target_configdirs BISON CC_FOR_BUILD LEX MAKEINFO YACC config_shell AR ncn_cv_AR AS ncn_cv_AS DLLTOOL ncn_cv_DLLTOOL LD ncn_cv_LD NM ncn_cv_NM RANLIB ncn_cv_RANLIB WINDRES ncn_cv_WINDRES OBJCOPY ncn_cv_OBJCOPY OBJDUMP ncn_cv_OBJDUMP CC CXX CFLAGS CFLAGS_FOR_BUILD CXXFLAGS DEFAULT_YACC DEFAULT_LEX DEFAULT_M4 AR_FOR_TARGET ncn_cv_AR_FOR_TARGET GAS_FOR_TARGET ncn_cv_GAS_FOR_TARGET DLLTOOL_FOR_TARGET ncn_cv_DLLTOOL_FOR_TARGET LD_FOR_TARGET ncn_cv_LD_FOR_TARGET NM_FOR_TARGET ncn_cv_NM_FOR_TARGET RANLIB_FOR_TARGET ncn_cv_RANLIB_FOR_TARGET WINDRES_FOR_TARGET ncn_cv_WINDRES_FOR_TARGET GCC_FOR_TARGET FLAGS_FOR_TARGET CC_FOR_TARGET GCJ_FOR_TARGET CXX_FOR_TARGET RAW_CXX_FOR_TARGET CXX_FOR_TARGET_FOR_RECURSIVE_MAKE RAW_CXX_FOR_TARGET_FOR_RECURSIVE_MAKE MAINTAINER_MODE_TRUE MAINTAINER_MODE_FALSE MAINT LIBOBJS LTLIBOBJS'
ac_subst_files='maybe_dependencies serialization_dependencies host_makefile_frag target_makefile_frag alphaieee_frag ospace_frag'

# Initialize some variables set by options.
ac_init_help=
ac_init_version=false
# The variables have the same names as the options, with
# dashes changed to underlines.
cache_file=/dev/null
exec_prefix=NONE
