## Process this file with automake to produce Makefile.in

AUTOMAKE_OPTIONS = 1.4

INCLUDES = -I$(top_srcdir)/tools/sfio

LIBS =

noinst_HEADERS = 

noinst_LTLIBRARIES = libsfiof.la
man_MANS = 

libsfiof_la_SOURCES = _sfclrerr.c _sfdlen.c _sfeof.c _sferror.c \
	_sffileno.c _sfgetc.c _sfllen.c _sfputc.c _sfputd.c \
	_sfputl.c _sfputm.c _sfputu.c _sfslen.c _sfstacked.c \
	_sfulen.c _sfvalue.c

${top_builddir}/FEATURE/sfio:  ${top_srcdir}/tools/sfio/features/sfio
	mkdir -p ${top_builddir}/FEATURE
	@KSH@ $(top_srcdir)/iffe - set cc $(CC) $(CCMODE) $(CXFLAGS) : run ${top_srcdir}/tools/sfio/features/sfio > $@

$(libsfiof_la_OBJECTS): ${top_builddir}/FEATURE/sfio

DISTCLEANFILES = ${top_builddir}/FEATURE/sfio

EXTRA_DIST = $(man_MANS) Makefile.old
