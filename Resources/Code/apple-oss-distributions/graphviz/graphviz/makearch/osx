X11LIB=/usr/X11R6/lib
X11INC=/usr/X11R6/include

CC=cc
YACC=yacc
FLEX=flex -8
SED=sed
LN=ln
RM=rm -f
RANLIB=ranlib
MKDIR=mkdir
MKPATH=mkdir -p
AR=ar
SHLIB_LD=ld -shared
SHLIB_SUFFIX=.so
MV=mv
CP=cp
AWK=awk
SORT=sort
INSTALL=cp

CCFLAGS=-O -D__Mac_OSX__=1 -DHAVE_FILE_R=1 -DHAVE_TERMIOS_H=1
LDFLAGS=-Wl,-all_load
# LEFTYLIBS=-lSM -lICE
TCLDOTLIBS=-ldl

.c.o:
	$(CC) -c $(CCFLAGS) $(DEFINES) $(INCS) $*.c -o $@

