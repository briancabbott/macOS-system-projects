<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/j2ee/dtds/ejb-jar_2_0.dtd">

<ejb-jar>
   <description>Stateless Session Bean used to test RMI/IIOP parameter passing</description>
   <display-name>RMI/IIOP parameter passing test</display-name>
   <enterprise-beans>
      <session>
         <display-name>Stateless IIOP Session</display-name>
         <ejb-name>StatelessIIOPSession</ejb-name>
         <home>org.jboss.test.iiop.interfaces.StatelessSessionHome</home>
         <remote>org.jboss.test.iiop.interfaces.StatelessSession</remote>
         <ejb-class>org.jboss.test.iiop.ejb.StatelessSessionBean</ejb-class>
         <session-type>Stateless</session-type>
         <transaction-type>Container</transaction-type>
      </session>
   </enterprise-beans>
</ejb-jar>
