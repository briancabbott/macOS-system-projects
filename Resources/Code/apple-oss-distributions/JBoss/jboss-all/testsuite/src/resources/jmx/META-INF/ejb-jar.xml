<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/j2ee/dtds/ejb-jar_2_0.dtd">

<ejb-jar>
     <description>jBoss test jmx datasource start application </description>
     <display-name>TestJMXDataSource</display-name>
     <enterprise-beans>
       <session>
	 <ejb-name>TestDataSource</ejb-name>
	 <home>org.jboss.test.jmx.interfaces.TestDataSourceHome</home>
	 <remote>org.jboss.test.jmx.interfaces.TestDataSource</remote>
	 <ejb-class>org.jboss.test.jmx.ejb.TestDataSourceBean</ejb-class>
	 <session-type>Stateless</session-type>
	 <transaction-type>Container</transaction-type>
       </session>
     </enterprise-beans>
   </ejb-jar>
