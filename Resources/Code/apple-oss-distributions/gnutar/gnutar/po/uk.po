msgstr "ключове_слово[[:]=значення][,ключове_слово[[:]=значення], ...]\""

#: src/tar.c:574
msgid "control pax keywords"
msgstr "керування ключовими словами pax"

#: src/tar.c:575
msgid "TEXT"
msgstr "ТЕКСТ"

#: src/tar.c:576
msgid ""
"create archive with volume name TEXT; at list/extract time, use TEXT as a "
