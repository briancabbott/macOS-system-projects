# Check that target clashes are diagnosed.

required=gcc
. ./defs || exit 1

set -e

cat >> configure.in << 'END'
AC_PROG_CC
END

cat > Makefile.am << 'END'
bin_PROGRAMS = ctags
END

$ACLOCAL
$AUTOCONF
AUTOMAKE_fails -a
grep 'redefinition.*ctags' stderr
