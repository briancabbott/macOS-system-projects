<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Disabled</key>
	<true/>
	<key>Label</key>
	<string>org.clamav.freshclam-init</string>
	<key>Program</key>
	<string>/Applications/Server.app/Contents/ServerRoot/usr/bin/freshclam</string>
	<key>ProgramArguments</key>
	<array>
		<string>/Applications/Server.app/Contents/ServerRoot/usr/bin/freshclam</string>
		<string>--quiet</string>
	</array>
	<key>StandardErrorPath</key>
	<string>/Library/Logs/Mail/freshclam.log</string>
	<key>ServiceIPC</key>
	<false/>
	<key>UserName</key>
	<string>_clamav</string>
	<key>RunAtLoad</key>
	<true/>
</dict>
</plist>
