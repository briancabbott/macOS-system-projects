; RUN: rm -rf %t && mkdir -p %t/outdir
; RUN: cp -r %S/Inputs/VFS.framework %t/outdir/
; RUN: sed -e "s:INPUT_DIR:%S/Inputs:g" -e "s:OUT_DIR:%t/outdir:g" -e "s:EXTERNAL_NAMES:true:" %S/Inputs/vfs.yaml > %t/vfs.external.yaml
; RUN: sed -e "s:INPUT_DIR:%S/Inputs:g" -e "s:OUT_DIR:%t/outdir:g" -e "s:EXTERNAL_NAMES:false:" %S/Inputs/vfs.yaml > %t/vfs.yaml

; RUN: %tapi installapi -ivfsoverlay %t/vfs.external.yaml -target x86_64-apple-macosx.10.15 -install_name /System/Library/Frameworks/VFS.framework/VFS -isysroot %sysroot %t/outdir/VFS.framework -o %t/VFS.tbd 2>&1 | FileCheck -check-prefix=CHECK-DIAG-EXTERNAL %s
; CHECK-DIAG-EXTERNAL: {{.*}}Inputs{{.*}}VFS.h:{{[0-9]*:[0-9]*}}: warning: incompatible pointer
; RUN: %tapi installapi -ivfsoverlay %t/vfs.yaml -target x86_64-apple-macosx.10.15 -install_name /System/Library/Frameworks/VFS.framework/VFS -isysroot %sysroot %t/outdir/VFS.framework -o %t/VFS.tbd 2>&1 | FileCheck -check-prefix=CHECK-DIAG %s
; CHECK-DIAG-NOT: Inputs
