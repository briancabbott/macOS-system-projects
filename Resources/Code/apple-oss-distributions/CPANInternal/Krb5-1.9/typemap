INPUT

T_PTROBJ_NU
	if ($arg == &PL_sv_undef) {
		$var = 0;
	}
	else if (sv_isa($arg, \"${ntype}\")) {
		IV tmp = SvIV((SV*)SvRV($arg));
		$var = ($type) tmp;
	}
	else {
		croak(\"$var is not of type ${ntype}\");
	}

OUTPUT

T_PTROBJ_NU
	sv_setref_pv($arg, \"${ntype}\", (void*)$var);
